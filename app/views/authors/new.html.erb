<h1>New Author</h1>

<%= form_for :authors, url: new_author_path do |f| %>
    <p>
      <%= f.label :first_Name %><br>
      <%= f.text_field :first_name %>
    </p>

    <p>
      <%= f.label :last_name %><br>
      <%= f.text_area :last_name %>
    </p>

    <p>
      <%= f.label :homepage %><br>
      <%= f.text_area :homepage%>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>